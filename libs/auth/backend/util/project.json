{
  "name": "auth-backend-util",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/auth/backend/util/src",
  "projectType": "library",
  "tags": ["scope:backend", "type:util", "domain:auth"],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/auth/backend/util",
        "tsConfig": "libs/auth/backend/util/tsconfig.lib.json",
        "packageJson": "libs/auth/backend/util/package.json",
        "main": "libs/auth/backend/util/src/index.ts",
        "assets": ["libs/auth/backend/util/*.md"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/auth/backend/util/jest.config.ts"
      }
    }
  }
}
