{
  "name": "data-access-backend-roles",
  "$schema": "../../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/shared/data-access/backend/roles/src",
  "projectType": "library",
  "tags": ["scope:backend", "type:data-access", "domain:shared"],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/shared/data-access/backend/roles",
        "tsConfig": "libs/shared/data-access/backend/roles/tsconfig.lib.json",
        "packageJson": "libs/shared/data-access/backend/roles/package.json",
        "main": "libs/shared/data-access/backend/roles/src/index.ts",
        "assets": ["libs/shared/data-access/backend/roles/*.md"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/shared/data-access/backend/roles/jest.config.ts"
      }
    }
  }
}
